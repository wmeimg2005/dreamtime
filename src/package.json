    "@nuxtjs/tailwindcss": "^1.2.0",
    "@octokit/rest": "^16.35.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-watch": "^7.0.0",
    "cross-env": "^6.0.3",
    "delay-cli": "^1.1.0",
    "electron": "^7.1.1",
    "electron-builder": "^22.1.0",
    "electron-devtools-installer": "^2.2.4",
