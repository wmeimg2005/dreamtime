    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-node": ">=10.0.0",
    "eslint-plugin-nuxt": ">=0.5.0",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-standard": ">=4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "rollbar-sourcemap-webpack-plugin": "^2.5.1",
    "sass": "^1.23.7",
